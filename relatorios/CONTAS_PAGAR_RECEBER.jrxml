	<queryString>
		<![CDATA[SELECT C.CD_CONTA, C.DS_CONTA, C.CD_FORMA, F.DS_FORMA, C.VL_CONTA,
TO_CHAR(C.DT_VENCIMENTO, 'DD/MM/YYYY') AS DT_VENC,
TO_CHAR(C.DT_PAGAMENTO, 'DD/MM/YYYY') AS DT_PGTO,
CASE WHEN C.TIPO_CONTA = 'P' THEN 'A PAGAR' ELSE
