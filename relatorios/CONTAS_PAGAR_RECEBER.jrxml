	<variable name="Total" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{vl_conta}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="TotalContasPagar" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{tipo}.equals("A PAGAR") ? $F{vl_conta} : 0)]]></variableExpression>
	</variable>
	<variable name="TotalContasReceber" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{tipo}.equals("A RECEBER") ? $F{vl_conta} : 0)]]></variableExpression>
	</variable>
	<variable name="ReceberPagas" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{sit}.equals("PAGA") && $F{tipo}.equals("A RECEBER")? $F{vl_conta}: 0)]]></variableExpression>
	</variable>
	<variable name="ReceberNPagas" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{TotalContasReceber} - $V{ReceberPagas}]]></variableExpression>
	</variable>
	<variable name="PagarPagas" class="java.math.BigDecimal">
		<variableExpression><![CDATA[($F{sit}.equals("PAGA") && $F{tipo}.equals("A PAGAR")? $F{vl_conta}: 0)]]></variableExpression>
	</variable>
	<variable name="PagarNPagas" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{TotalContasPagar} - $V{PagarPagas}]]></variableExpression>
	</variable>
